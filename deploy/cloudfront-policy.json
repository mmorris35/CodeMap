{
  "CachePolicyName": "CodeMapAPINoCache",
  "CachePolicyConfig": {
    "DefaultTTL": 0,
    "MaxTTL": 0,
    "MinTTL": 0,
    "ParametersInCacheKeyAndForwardedToOrigin": {
      "EnableAcceptEncodingGzip": true,
      "EnableAcceptEncodingBrotli": true,
      "QueryStringsConfig": {
        "QueryStringBehavior": "all"
      },
      "HeadersConfig": {
        "HeaderBehavior": "allViewerAndWhitelistCloudFront"
      },
      "CookiesConfig": {
        "CookieBehavior": "all"
      }
    },
    "Comment": "CodeMap API policy - no caching for dynamic responses. Allows compression (gzip, brotli) and forwards all headers and query strings to origin."
  }
}
